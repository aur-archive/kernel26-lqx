_basekernel=2.6.39
pkgver=${_basekernel}.3
pkgrel=3
_kernelname=-lqx
pkgname=kernel26${_kernelname}
arch=('i686' 'x86_64')
pkgdesc="OBSOLETE PACKAGE - Use linux-lqx (https://aur.archlinux.org/packages.php?ID=51332) instead."
license=('GPL2')
groups=('base')
url="https://aur.archlinux.org/packages.php?ID=51332"
backup=(etc/mkinitcpio.d/${pkgname}.preset)

build() {
  msg "OBSOLETE PACKAGE - Use linux-lqx (https://aur.archlinux.org/packages.php?ID=51332) instead!"
  msg2 "See https://www.archlinux.org/news/changes-to-kernel-package-and-filenames/ for details"
  exit 1
}

